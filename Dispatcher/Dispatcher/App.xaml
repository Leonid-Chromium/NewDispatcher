             xmlns:local="clr-namespace:Dispatcher"
             StartupUri="AuthorizationWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Style/LeonidDictionary.xaml"/>
                
                <ResourceDictionary Source="Style/SecondRoundStyle.xaml"/>
