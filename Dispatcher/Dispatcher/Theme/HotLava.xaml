<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Tranceparent" Color="#00000000"/>

    <SolidColorBrush x:Key="BackFirstLevel" Color="#880000"/>
    <SolidColorBrush x:Key="BackSecondLevel" Color="#9E0000"/>
    <SolidColorBrush x:Key="BackThirdLevel" Color="#FD0000"/>

    <SolidColorBrush x:Key="FrontFirstLevel" Color="#384FA1"/>
    <SolidColorBrush x:Key="FrontSecondLevel" Color="#C4C4C4"/>
    <SolidColorBrush x:Key="FrontThirdLevel" Color="#FAFF00"/>
</ResourceDictionary>