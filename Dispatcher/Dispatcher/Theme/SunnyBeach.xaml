<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Tranceparent" Color="#00000000"/>

    <SolidColorBrush x:Key="BackFirstLevel" Color="#A66100"/>
    <SolidColorBrush x:Key="BackSecondLevel" Color="#FFB040"/>
    <SolidColorBrush x:Key="BackThirdLevel" Color="#FFC573"/>

    <SolidColorBrush x:Key="FrontFirstLevel" Color="#FF4100"/>
    <SolidColorBrush x:Key="FrontSecondLevel" Color="#C4C4C4"/>
    <SolidColorBrush x:Key="FrontThirdLevel" Color="#006363"/>
</ResourceDictionary>